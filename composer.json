{
    "name": "anovsiradj/sqlrun",
    "description": "migrasi SQL satu arah",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "anovsiradj",
            "email": "anov.siradj22@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "anovsiradj\\sqlrun\\drivers\\": "./drivers",
            "anovsiradj\\sqlrun\\runners\\": "./runners"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "sqlrun\\yii2\\": "./tests/yii2",
            "sqlrun\\laravel\\": "./tests/laravel"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "ext-pdo": "*",
        "symfony/dotenv": "~5",
        "symfony/var-dumper": "~5",
        "yiisoft/yii2": "@dev",
        "laravel/framework": "~8"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
